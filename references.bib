  keywords = {Alcohol Comprehensive Health Enhancement Support System (A-CHESS) Program,Alcohol use disorders (AUDs),Alcoholism,Cell Phone,continuing care,Health Promotion,Humans,information and communication technologies (ICTs),literature review,risk2_protocol_paper,Self Care,self-management,smartphone,Social Support,telecommunication technology,treatment method}
}

@article{gustafsonEHealthSystemSupporting2013,
  title = {An {{eHealth}} System Supporting Palliative Care for Patients with Non-Small Cell Lung Cancer: A Randomized Trial},
  shorttitle = {An {{eHealth}} System Supporting Palliative Care for Patients with Non-Small Cell Lung Cancer},
  author = {Gustafson, David H. and DuBenske, Lori L. and Namkoong, Kang and Hawkins, Robert and Chih, Ming-Yuan and Atwood, Amy K. and Johnson, Roberta and Bhattacharya, Abhik and Carmack, Cindy L. and Traynor, Anne M. and Campbell, Toby C. and Buss, Mary K. and Govindan, Ramaswamy and Schiller, Joan H. and Cleary, James F.},
  year = 2013,
  month = may,
  journal = {Cancer},
  volume = {119},
  number = {9},
  pages = {1744--1751},
  issn = {1097-0142},
  doi = {10.1002/cncr.27939},
  abstract = {BACKGROUND: In this study, the authors examined the effectiveness of an online support system (Comprehensive Health Enhancement Support System [CHESS]) versus the Internet in relieving physical symptom distress in patients with non-small cell lung cancer (NSCLC). METHODS: In total, 285 informal caregiver-patient dyads were assigned randomly to receive, for up to 25 months, standard care plus training on and access to either use of the Internet and a list of Internet sites about lung cancer (the Internet arm) or CHESS (the CHESS arm). Caregivers agreed to use CHESS or the Internet and to complete bimonthly surveys; for patients, these tasks were optional. The primary endpoint-patient symptom distress-was measured by caregiver reports using a modified Edmonton Symptom Assessment Scale. RESULTS: Caregivers in the CHESS arm consistently reported lower patient physical symptom distress than caregivers in the Internet arm. Significant differences were observed at 4 months (P = .031; Cohen d = .42) and at 6 months (P = .004; d = .61). Similar but marginally significant effects were observed at 2 months (P = .051; d = .39) and at 8 months (P = .061; d = .43). Exploratory analyses indicated that survival curves did not differ significantly between the arms (log-rank P = .172), although a survival difference in an exploratory subgroup analysis suggested an avenue for further study. CONCLUSIONS: The current results indicated that an online support system may reduce patient symptom distress. The effect on survival bears further investigation.},
  langid = {english},
  pmcid = {PMC3684251},
  pmid = {23355273},
  keywords = {Adult,Aged,Carcinoma Non-Small-Cell Lung,Caregivers,Humans,Internet,Lung Neoplasms,Middle Aged,Palliative Care,risk2_protocol_paper},
  file = {C\:\\Users\\kpaquette2\\Zotero\\storage\\29M5E49S\\Gustafson et al. - 2013 - An eHealth system supporting palliative care for p.pdf;C\:\\Users\\kpaquette2\\Zotero\\storage\\9HCE5AM5\\Gustafson et al. - 2013 - An eHealth system supporting palliative care for p.pdf}
}

@article{gustafsonExplicatingEvidencebasedTheoretically2011,
  title = {Explicating an Evidence-Based, Theoretically Informed, Mobile Technology-Based System to Improve Outcomes for People in Recovery for Alcohol Dependence},
  author = {Gustafson, David H. and Shaw, Bret R. and Isham, Andrew and Baker, Timothy and Boyle, Michael G. and Levy, Michael},
