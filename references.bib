  keywords = {Adult,Aftercare,Continuity of Patient Care,Counseling,Feasibility Studies,Female,Humans,Male,Patient Compliance,Remote Consultation,Residential Treatment,risk2_protocol_paper,Substance Abuse Treatment Centers,Substance-Related Disorders,Telephone},
