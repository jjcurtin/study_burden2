@article{breslinComparisonBriefLong2000,
  title = {A Comparison of a Brief and Long Version of the {{Situational Confidence Questionnaire}}},
  author = {Breslin, F. C. and Sobell, L. C. and Sobell, M. B. and Agrawal, S.},
