@article{hussongSpecifyingRelationsAffect2001,
  title = {Specifying the Relations between Affect and Heavy Alcohol Use among Young Adults},
  author = {Hussong, A. M. and Hicks, R. E. and Levy, S. A. and Curran, P. J.},
